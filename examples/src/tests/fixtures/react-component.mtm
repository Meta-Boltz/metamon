---
name: TestReactComponent
target: react
description: A test React component
props:
  - name: title
    type: string
    required: true
  - name: count
    type: number
    default: 0
---

<div className="test-react-component">
  <h2>{props.title}</h2>
  <p>Count: {props.count}</p>
  <button onClick={() => setCount(count + 1)}>
    Increment
  </button>
</div>