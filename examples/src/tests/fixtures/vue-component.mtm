---
name: TestVueComponent
target: vue
description: A test Vue component
props:
  title:
    type: String
    required: true
  count:
    type: Number
    default: 0
---

<template>
  <div class="test-vue-component">
    <h2>{{ title }}</h2>
    <p>Count: {{ count }}</p>
    <button @click="increment">
      Increment
    </button>
  </div>
</template>

<script setup>
const increment = () => {
  count.value++;
};
</script>