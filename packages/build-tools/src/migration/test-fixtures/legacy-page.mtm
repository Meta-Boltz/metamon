$count = createSignal(0)
$message = createSignal('Hello World')

$mount = () => {
  console.log('Page mounted')
}

$increment = () => {
  $count++
}

<div class="legacy-page">
  <h1>{$message}</h1>
  <button onClick={$increment}>Count: {$count}</button>
</div>