{
  "name": "mtm-framework",
  "version": "1.0.0",
  "description": "MTM (Meta-Template-Metamon) Framework - Write once, compile to React, Vue, Svelte, SolidJS, or Pure HTML/JS",
  "main": "src/mtm-compiler/cli.js",
  "bin": {
    "mtm": "./src/mtm-compiler/cli.js"
  },
  "scripts": {
    "compile": "node src/mtm-compiler/cli.js compile",
    "compile-all": "node src/mtm-compiler/cli.js compile-all",
    "test": "node test/simple-test.js",
    "demo": "npm run compile-examples && npm run serve-examples",
    "compile-examples": "node src/mtm-compiler/cli.js compile-all examples/mtm-components compiled",
    "serve-examples": "node examples/serve-compiled.js",
    "dev": "npm run compile-examples && npm run serve-examples"
  },
  "keywords": [
    "meta-framework",
    "react",
    "vue",
    "svelte",
    "solidjs",
    "compiler",
    "template",
    "reactive",
    "cross-framework"
  ],
  "author": "MTM Framework Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "jest": "^29.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mtm-framework/mtm"
  },
  "bugs": {
    "url": "https://github.com/mtm-framework/mtm/issues"
  },
  "homepage": "https://mtm-framework.dev"
}